  },
  "1265": {
    "p": "PERSONAL USE ONLY, AND (2) ARE NOT DISTRIBUTED OR USED",
    "len": 10
  },
  "1266": {
    "p": "COMMERCIALLY. PROHIBITED COMMERCIAL DISTRIBUTION INCLUDES BY ANY",
